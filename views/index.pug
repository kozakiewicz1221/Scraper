html
  head
    title= title
    style
    link(rel="stylesheet" type="text/css" href="https://unpkg.com/tailwindcss@^1.0/dist/tailwind.min.css")
  body
    div.p-8
      h1.uppercase.text-teal-500.text-6xl.font-black cryptonews.com
      ul.pl-5.flex.flex-wrap
        each val in output
          div(class="w-1/3 p-3")
            a(href=val.fullurl)
              img(src=val.img class="w-full")
              li.my-2.uppercase.text-teal-700.text-2xl= val.title
